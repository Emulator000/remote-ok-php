{
    "name": "alsoknownasdrew/remote-ok-php",
    "description": "Remoteok.io API PHP Client ",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "alsoknownasdrew",
            "email": "andriy.frankevych@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Alsoknownasdrew\\RemoteOK\\": "src/",
            "Alsoknownasdrew\\RemoteOKTest\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "psr/http-message": "^1.0",
        "guzzlehttp/guzzle": "^7.1",
        "laminas/laminas-diactoros": "^2.4"
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.52",
        "phpunit/phpunit": "^9.4"
    },
    "scripts": {
        "test-unit": "./vendor/bin/phpunit --testsuite Unit",
        "static-check": "vendor/bin/phpstan analyse -c phpstan.neon"
    }
}
